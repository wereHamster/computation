{
  "linter": {
    "rules": {
      "complexity": {
        "useArrowFunction": "off"
      },
      "style": {
        "useSingleVarDeclarator": "off",
        "useTemplate": "off",
        "noUselessElse": "off"
      },
      "suspicious": {
        "noExplicitAny": "off",
        "noThenProperty": "off"
      }
    }
  }
}
